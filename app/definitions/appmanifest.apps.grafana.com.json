                                                },
                                                "status": {
                                                    "enum": [
                                                        "success",
                                                        "failure"
                                                    ],
                                                    "type": "string"
                                                }
                                            },
                                            "required": [
                                                "status"
                                            ],
                                            "type": "object"
                                        },
                                        "description": "Resources contains the status of each resource type created or updated in the API server\nas a result of the AppManifest.",
                                        "type": "object"
                                    }
                                },
                                "required": [
                                    "resources"
                                ],
                                "type": "object",
                                "x-kubernetes-preserve-unknown-fields": true
